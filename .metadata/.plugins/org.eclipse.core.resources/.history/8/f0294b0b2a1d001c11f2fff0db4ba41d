package question_4;

public class Main {

	public static void main(String[] args) {
		Rectangle r1 = new Rectangle ();
		Rectangle r2 = new Rectangle ();
		System.out.println("area of rectangle r1: " +r1.area(4, 5));
		System.out.println("area of rectangle r2: " +r2.area(5, 8));
	}
}

class Rectangle {

	public int area(int i, int j) {
		int area = i * j;
		return area;
	}
}
